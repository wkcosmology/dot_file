{
    "coc.preferences.messageLevel": "warning",
    "coc.preferences.hoverTarget": "float",
    "coc.preferences.previewAutoClose": false,

    "signature.enable": true,
    "signature.target": "echo",

    "coc.source.buffer.enable": true,
    "coc.source.around.enable": true,

    "snippets.ultisnips.enable": true,
    "snippets.ultisnips.usePythonx": true,
    "snippets.snipmate.enable": false,

    "diagnostic.enable": false,
    "diagnostic.enableSign": false,
    "diagnostic.messageTarget": "float",

    "clangd.arguments": [
        "--completion-style=detailed",
        "--all-scopes-completion=false",
        "--pretty",
        "--header-insertion=never",
        "--index=true"
    ],
    "clangd.disableDiagnostics": true,
    "clangd.fallbackFlags": ["-std=c++17"],

    "jedi.enable": true,
    "jedi.startupMessage": false,
    "jedi.markupKindPreferred": "markdown",
    "jedi.trace.server": "off",
    "jedi.executable.command": "/Users/wangk/anaconda3/bin/jedi-language-server",
    "jedi.executable.args": [],
    "jedi.completion.disableSnippets": false,
    "jedi.diagnostics.enable": true,
    "jedi.diagnostics.didOpen": false,
    "jedi.diagnostics.didChange": false,
    "jedi.diagnostics.didSave": true,
    "jedi.workspace.extraPaths": [],

    "lua.enable": true,
    "Lua.diagnostics.enable": false,

    "yank.highlight.enable": false,
    "yank.list.maxsize": 50,
    "yank.enableCompletion": false,

    "list.insertMappings": {
        "<C-r>": "do:refresh",
        "<C-c>": "do:exit"
    },

    "suggest.noselect": true,
    "suggest.enablePreselect": false,
    "suggest.maxCompleteItemCount": 20,
    "suggest.floatEnable": false,
    "suggest.enablePreview": false,
    "suggest.echodocSupport": true,
    "suggest.minTriggerInputLength": 2,
    "suggest.keepCompleteopt": false,
    "suggest.detailField": "preview",
    "suggest.labelMaxLength": 100,
    "suggest.removeDuplicateItems": true,
    "suggest.disableMenu": true,
    "suggest.autoTrigger": "always",
    "suggest.completionItemKindLabels": {
        "keyword": "  [Keyword]",
        "variable": "  [Variable]",
        "value": "  [Value]",
        "operator": "  [Operator]",
        "function": "  [Function]",
        "reference": "渚  [Reference]",
        "constant": "  [Constant]",
        "method": "  [Method]",
        "struct": "פּ  [Struct]",
        "class": "  [Class]",
        "interface": "  [Interface]",
        "text": "  [Text]",
        "enum": "  [Enum]",
        "enumMember": "  [EnumMember]",
        "module": "  [Module]",
        "color": "  [Color]",
        "property": "  [Property]",
        "field": "料  [Field]",
        "unit": "  [Unit]",
        "event": "鬒  [Event]",
        "file": "  [File]",
        "folder": "  [Folder]",
        "snippet": "  [Snippet]",
        "typeParameter": "  [TypeParameter]",
        "default": "  [Default]"
    },

    "css.enable": true,

}
